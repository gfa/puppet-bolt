[<%= @name %>]
enabled = <%= @enabled %>
<% if not @portrange.nil? -%>port = <%= @portrange %>
<% end -%>
<% if not @filter.nil? -%>filter = <%= @filter %>
<% end -%>
<% if not @logpath.nil? -%>logpath = <%= @logpath %>
<% end -%>
<% if not @protocol.nil? -%>protocol = <%= @protocol %>
<% end -%>
<% if not @maxretry.nil? -%>maxretry = <%= @maxretry %>
<% end -%>
<% if not @findtime.nil? -%>findtime = <%= @findtime %>
<% end -%>
<% if not @action.nil? -%>action = <%= @action %>
<% end -%>
<% if not @ignorecommand.nil? -%>ignorecommand = <%= @ignorecommand %>
<% end -%>
<% if not @banaction.nil? -%>banaction = <%= @banaction %>
<% end -%>
<% if not @bantime.nil? -%>bantime = <%= @bantime %>
<% end -%>
<% if not @ignoreip.empty? -%>ignoreip = <%= @ignoreip.join(' ') %>
<% end -%>
<% if not @backend.nil? -%>backend = <%= @backend %>
<% end -%>
<% if not @additional_options.empty?
  @additional_options.each do |opt,value| -%>
<%= opt %> = <%= value %>
<% end
end -%>

